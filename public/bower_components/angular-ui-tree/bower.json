{
  "name": "angular-ui-tree",
  "version": "2.1.5",
  "homepage": "http://angular-ui-tree.github.io/website",
  "authors": [
    "Jim Liu <https://github.com/JimLiu>",
    "Voles <https://github.com/Voles>",
